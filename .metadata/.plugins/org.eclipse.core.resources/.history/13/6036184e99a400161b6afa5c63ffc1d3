package cn.ucai.day16.io;

import java.io.File;
import java.io.IOException;

/**
 * File：
 * 
 */
public class TestFile {
	public static void main(String[] args) {
		File file = new File("E:/abc/","a.txt");
		
		
		
		
		try {
			file.createNewFile();
			System.out.println("文件创建成功");
		} catch (IOException e) {
			System.out.println("文件创建失败:"+e.getMessage());
		}
	}
}
