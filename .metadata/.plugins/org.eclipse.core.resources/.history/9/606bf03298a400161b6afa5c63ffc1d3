package cn.ucai.day16.io;

import java.io.File;
import java.io.IOException;

/**
 * File：
 * 
 */
public class TestFile {
	public static void main(String[] args) {
		// File类是硬盘上文件或文件夹的抽象表示，跟具体文件存在不存在没有关系。
		File file = new File("E:/abc/a.txt");
		try {
			file.createNewFile();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
