package cn.ucai.day16.io;

import java.io.File;
import java.io.IOException;

/**
 * createNewFile:当文件存在时，不新建，文件不存在才新建。
 * 
 */
public class TestFile4 {
	public static void main(String[] args) {
		File file = new File("E:/abc/a.txt");
		if(file.exists()){
			file.delete();
		}
		try {
			file.createNewFile();
		} catch (IOException e) {
			e.printStackTrace();
		}
		
		
	}
}
