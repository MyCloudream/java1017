package cn.ucai.day28.sort;

/**
 * ÉýÐò
 * Ã°ÅÝÅÅÐò
 */
public class TestBubble {
	public static void main(String[] args) {
		int[] niArr = {3, 44, 38, 5, 47, 15, 36, 26, 27, 2, 46, 4, 19, 50, 48};
		// µÚÒ»ÂÖ
		for(int i=0;i<niArr.length;i++){
			if(niArr[i]>niArr[i+1]){
				int temp = niArr[i];
				niArr[i] = niArr[i+1];
				niArr[i+1] = temp;
			}
		}
	}
}
